<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        {# Run `composer require symfony/webpack-encore-bundle`
           and uncomment the following Encore helpers to start using Symfony UX #}
        <link rel="icon" href="{{ asset('src/img/general-electrics.png') }}"  type="image/png" />
        <link rel="stylesheet" href="{{ asset('src/css/bootstrap.css') }}">
        {% block stylesheets %}
            {#{{ encore_entry_link_tags('app') }}#}
        {% endblock %}

        <script src="{{asset('src/js/bootstrap.js')}}" type="text/javascript"></script>
        {% block javascripts %}
            {#{{ encore_entry_script_tags('app') }}#}
        {% endblock %}
    </head>
    <body>
        {{ include('front/comunes/navbar.html.twig') }}
        <div class="container">
            {% block body %}{% endblock %}
        </div>
    </body>
</html>
